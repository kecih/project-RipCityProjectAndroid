<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ripcity" >
    <uses-permission android:name="android.permission.INTERNET"/>


    <application

        android:allowBackup="true"
        android:icon="@drawable/portland"
        android:label="Golden State Warrior"
        android:roundIcon="@drawable/portland"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ripcity.ui.main.ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            >

        </activity>

        <activity android:name="com.example.ripcity.ui.main.ui.LoginAdmin"
            >

        </activity>

        <activity android:name="com.example.ripcity.ui.main.ui.SplashScreenActivity"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>